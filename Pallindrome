import java.util.Scanner;
public class Main{
  public static void main(String[] args){
    int a, sum =0, rev;
    Scanner sc = new Scanner(System.in);
    a = sc.nextInt();
    while(1){
        a = a%10;
        rev = rev +a;
        a = a/10;
        }
        if(a == rev){
        System.out.println("");
        else{
        System.out.println("");
        }
        }
        }
